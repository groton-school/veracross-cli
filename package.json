{
  "name": "veracross-cli",
  "homepage": "https://github.com/groton-school/veracross-cli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/groton-school/veracross-cli.git"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Seth Battis",
    "email": "sbattis@groton.org"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:paths": "monorepo-package-paths -w --author --repository --homepage",
    "build:compile": "lerna run build",
