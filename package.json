    "release:git": "git push --follow-tags",
    "release:publish": "pnpm publish -r --access public"
  },
  "devDependencies": {
    "@battis/eslint-config": "^2.0.9",
    "@battis/prettier-config": "^1.7.3",
    "@commitlint/config-conventional": "^20.3.1",
    "@commitlint/config-pnpm-scopes": "^20.1.0",
    "@examples/dev-command": "workspace:*",
    "@qui-cli/colors": "latest",
    "@types/node": "^25.0.9",
    "lerna": "^9.0.3",
    "monorepo-package-paths": "^0.4.3",
    "npm-run-all": "^4.1.5"
  },
  "packageManager": "pnpm@10.28.0"
}
